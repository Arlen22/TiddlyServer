{
  "name": "tiddlyserver",
  "version": "2.2.0-beta-4",
  "description": "Many TiddlyWiki, One Port",
  "homepage": "https://arlen22.github.io/tiddlyserver/",
  "repository": "github:Arlen22/TiddlyServer",
  "dependencies": {
    "tiddlywiki-production-client": "5.2.0",
    "tiddlywiki-production-server": "5.2.0"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/json5": "0.0.30",
    "@types/libsodium-wrappers": "^0.7.5",
    "@types/node": "^10.17.21",
    "@types/send": "^0.14.4",
    "@types/webpack": "^4.41.12",
    "@types/ws": "^6.0.1",
    "@types/xml-js": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "commander": "^6.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "formidable": "^1.1.1",
    "json-stable-stringify": "^1.0.1",
    "json5": "^2.1.0",
    "libsodium": "^0.7.4",
    "libsodium-wrappers": "^0.7.4",
    "morgan": ">=1.9.1",
    "prettier": "1.19.1",
    "rxjs": "^6.5.5",
    "send": "^0.17.1",
    "source-map-support": "^0.5.16",
    "ts-json-schema-generator": "^0.61.1",
    "ts-node": "^8.10.1",
    "tslib": "^1.11.1",
    "typescript": "^3.9.10",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "ws": "^7.2.1",
    "xml-js": "^1.6.11"
  },
  "scripts": {
    "test": "tsc && node dist/server --config settings.json --dry-run",
    "start": "tsc && node dist/server --config settings.json",
    "serve": "node dist/server --config settings.json",
    "debug": "node --inspect-brk dist/server --config settings.json",
    "server": "node dist/server",
    "index": "node dist/index",
    "schema": "tsc && node dist/server --gen-schema",
    "build": "tsc && webpack",
    "upload": "npm run build && (cd dist && npm publish)",
    "build-docs": "(cd ../tiddlyserver-website && npm run build && npm run push)",
    "install-build-tools": "npm install webpack webpack-cli typescript tslib ts-node source-map-support clean-webpack-plugin copy-webpack-plugin"
  },
