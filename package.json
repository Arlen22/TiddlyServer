{
  "name": "tiddlyserver",
  "version": "2.2.0-alpha-4",
  "description": "Many TiddlyWiki, One Port",
  "homepage": "https://arlen22.github.io/tiddlyserver/",
  "repository": "github:Arlen22/TiddlyServer",
  "bin": "./build/server.js",
  "main": "./build/src/server.js",
  "prodDependencies": [
    "tiddlywiki-production"
  ],
  "dependencies": {
    "@types/libsodium-wrappers": "^0.7.5",
    "@types/node": "^10.17.19",
    "@types/send": "^0.14.4",
    "@types/webpack": "^4.41.7",
    "@types/ws": "^6.0.1",
    "@types/yargs": "^15.0.4",
    "copy-webpack-plugin": "^5.1.1",
    "formidable": "^1.1.1",
    "json-stable-stringify": "^1.0.1",
    "json5": "^2.1.0",
    "libsodium": "^0.7.4",
    "libsodium-wrappers": "^0.7.4",
    "morgan": ">=1.9.1",
    "prettier": "1.19.1",
    "rxjs": "^6.5.5",
    "send": "^0.17.1",
    "source-map-support": "^0.5.16",
    "tiddlywiki-production": "5.1.22",
    "ts-json-schema-generator": "^0.61.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "ws": "^7.2.1",
    "yargs": "^15.1.0"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cp -v settings.json ./build/; node build/index",
    "assets": "cp -rv ./assets ./preflighter.js ./build",
    "build": "tsc && webpack && node build-schema.js && npm run assets && npm run build:final",
    "build:dev": "tsc && webpack",
    "build:clean": "rm -rf build && npm run build",
    "build:final": "rm -rf build/package.d.ts build/server.js build/server.d.ts build/src",
    "prettier": "prettier --write src/*.ts",
    "postinstall": "tsc && cp -rv ./assets ./build"
  },
  "author": "Arlen22@GitHub.com",
  "license": "MIT"
}